
public class Punto {

	// atributos
	private int posicionX;
	private int posicionY;

	// constructores

	public Punto(int posicionX, int posicionY) {

		this.posicionX = posicionX;
		this.posicionY = posicionY;

	}

	// getters

	public int getPosicionX() {
		return posicionX;
	}

	public int getPosicionY() {
		return posicionY;
	}
	
	//m√©todos de clase

	public boolean igual(Punto punto) {
		
		return posicionX == punto.getPosicionX() && posicionY == punto.getPosicionY();
		

	}

}
